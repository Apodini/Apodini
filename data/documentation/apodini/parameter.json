:)"]},{"title":"Instance Properties","identifiers":["doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/id-90me2","doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/projectedValue","doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/wrappedValue"]},{"title":"Type Aliases","identifiers":["doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/Option","doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/OptionKey"]},{"title":"Default Implementations","identifiers":["doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/Identifiable-Implementations"],"generated":true}],"variants":[{"paths":["\/documentation\/apodini\/parameter"],"traits":[{"interfaceLanguage":"swift"}]}],"kind":"symbol","primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"attribute","text":"@propertyWrapper"},{"kind":"text","text":" "},{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Parameter"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"Element"},{"kind":"text","text":"> "},{"kind":"keyword","text":"where"},{"kind":"text","text":" "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Decodable","preciseIdentifier":"s:Se"},{"kind":"text","text":", "},{"kind":"typeIdentifier","text":"Element"},{"kind":"text","text":" : "},{"kind":"typeIdentifier","text":"Encodable","preciseIdentifier":"s:SE"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"content","content":[{"anchor":"Overview","level":2,"type":"heading","text":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" property wrapper can be used to express input in different ways."},{"type":"text","text":" "},{"type":"text","text":"The internal name of the "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" and can be different to the external representation of the "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" by passing a string into the "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" property wrapper as a first optional argument."}]},{"type":"codeListing","syntax":"swift","code":["struct ExampleHandler: Handler {","    @Parameter var name: String","    @Parameter(\"repeat\") var times: Int","","    \/\/ ...","}"]},{"anchor":"Inference","level":2,"type":"heading","text":"Inference"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Apodini is designed so that different web API exporters for different protocols can infer as much information as possible about "},{"type":"codeVoice","code":"@Parameters"},{"type":"text","text":"."}]},{"anchor":"Parameters-Expressing-Request-Content","level":3,"type":"heading","text":"Parameters Expressing Request Content"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Non primitive types defined by the user that conform to "},{"type":"codeVoice","code":"Codable"},{"type":"text","text":" and are not "},{"type":"reference","isActive":true,"identifier":"https:\/\/developer.apple.com\/documentation\/swift\/losslessstringconvertible"},{"type":"text","text":" are automatically considered to be a request content for all protocols and types of middleware that are applicable to contain a request content such as an HTTP body. If there are multiple non primitive types marked with "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" the elements should be encoded in a wrapper enclosure."}]},{"anchor":"Single-Parameter-for-Request-Content","level":3,"type":"heading","text":"Single @Parameter for Request Content"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The following example showcases a simple "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" with one "},{"type":"codeVoice","code":"@Property"},{"type":"text","text":" wrapper around a non primitive type:"}]},{"type":"codeListing","syntax":"swift","code":["struct Bird: Codable {","    var name: String","    var age: Int","}","","struct ExampleHandler: Handler {","    @Parameter var bird: Bird","","    \/\/ ...","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"JSON Based APIs"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" can be exported as in a JSON based API by sending a POST request with the following JSON in the body of the HTTP request:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"name\": \"Swift\",","    \"age\": 42","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"gRPC"}]}]},{"type":"codeListing","syntax":"protobuf","code":["service Example {","    rpc Example (Bird) returns (\/*...*\/) {}","}","","message Bird {","    string name = 1;","    int32 age = 2;","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"GraphQL"}]}]},{"type":"codeListing","syntax":"graphql","code":["type Bird {","  name: String!","  age: Int!","}","","type Query {","    example(bird: Bird): \/*...*\/","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"WebSocket"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Exposing the "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" on a web socket interface requires the client to send the following message encoded as JSON:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"type\": \"Example\",","    \"parameters\": {","        \"name\": \"Swift\",","        \"age\": 42","    }","}"]},{"anchor":"Multiple-Parameter-s-for-Request-Content","level":3,"type":"heading","text":"Multiple @Parameter s for Request Content"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The following example showcases a "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" with multiple "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" property wrappers around a non primitive type:"}]},{"type":"codeListing","syntax":"swift","code":["struct Bird: Codable {","    var name: String","    var age: Int","}","","struct Plant: Codable {","    var name: String","    var height: Int","}","","struct ExampleHandler: Handler {","    @Parameter var bird: Bird","    @Parameter var plant: Plant","","    \/\/ ...","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"JSON Based APIs"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" can be exported as in a JSON based API by sending a POST request with the following JSON in the body of the HTTP request:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"bird\": {","        \"name\": \"Swift\",","        \"age\": 42","    },","    \"plant\": {","        \"name\": \"Rose\",","        \"height\": 10","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"gRPC"}]}]},{"type":"codeListing","syntax":"protobuf","code":["service Example {","    rpc Example (SearchRequest) returns (\/*...*\/) {}","}","","message SearchRequest {","    message Bird {","        string name = 1;","        int32 age = 2;","    }","","    message Plant {","        string name = 1;","        int32 height = 2;","    }","","    Bird bird = 1;","    Plant plant = 2;","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"GraphQL"}]}]},{"type":"codeListing","syntax":"graphql","code":["type Bird {","  name: String!","  age: Int!","}","","type Plant {","  name: String!","  height: Int!","}","","type Query {","    example(bird: Bird, plant: Plant): \/*...*\/","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"WebSocket"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Exposing the "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" on a web socket interface requires the client to send the following message encoded as JSON:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"type\": \"Example\",","    \"parameters\": {","        \"bird\": {","            \"name\": \"Swift\",","            \"age\": 42","        },","        \"plant\": {","            \"name\": \"Rose\",","            \"height\": 10","        }","    }","}"]},{"anchor":"Inferring-Query-Parameter-s","level":3,"type":"heading","text":"Inferring Query @Parameter s"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Some middleware types and protocols can expose parameters as lightweight parameters that can be part of a URI path such as query parameters found in the URI of RESTful and OpenAPI based interfaces. Apodini automatically exposes primitive types as lightweight parameters that conform to "},{"overridingTitleInlineContent":[{"type":"codeVoice","code":"LosslessStringConvertible"}],"isActive":true,"type":"reference","identifier":"https:\/\/developer.apple.com\/documentation\/swift\/losslessstringconvertible","overridingTitle":"LosslessStringConvertible"},{"type":"text","text":". Complex types such as custom types conforming to "},{"type":"codeVoice","code":"Codable"},{"type":"text","text":" can not be exposed using lightweight parameters."}]},{"type":"codeListing","syntax":"swift","code":["struct Bird: Codable {","    var name: String","    var age: Int","}","","struct ExampleHandler: Handler {","    \/\/ Is exposed as a content of a request","    @Parameter var bird: Bird","    \/\/ Indicates how often the bird should be returned in the response. Has a default value. Is exposed as a query parameter for interfaces that support it. ","    @Parameter(\"times\") var repeat: Int = 1","    \/\/ Has a default value of true and indicates if emojis should be used in the response. Is exposed as a query parameter for interfaces that support it.","    @Parameter var emoji: Bool = true","","    \/\/ ...","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"JSON Based APIs"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" can be exported as in a JSON based API by sending a POST request to "},{"type":"codeVoice","code":"...\/example?emoji=false&times=2"},{"type":"text","text":" or "},{"type":"codeVoice","code":"...\/example"},{"type":"text","text":" to assume the default value. The request includes the the following JSON in the body of the HTTP request:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"bird\": {","        \"name\": \"Swift\",","        \"age\": 42","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"gRPC"}]}]},{"type":"codeListing","syntax":"protobuf","code":["service Example {","    rpc Example (ExampleRequest) returns (\/*...*\/) {}","}","","message ExampleRequest {","    message Bird {","        string name = 1;","        int32 age = 2;","    }","","    Bird bird = 1;","    int32 times = 2;","    bool emoji = 3;","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"GraphQL"}]}]},{"type":"codeListing","syntax":"graphql","code":["type Bird {","  name: String!","  age: Int!","}","","type Query {","    example(bird: Bird, times: Int = 1, emoji: Bool = true): \/*...*\/","}"]},{"type":"paragraph","inlineContent":[{"type":"emphasis","inlineContent":[{"type":"text","text":"WebSocket"}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Exposing the "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" on a web socket interface requires the client to send the following message encoded as JSON:"}]},{"type":"codeListing","syntax":"json","code":["{","    \"type\": \"Example\",","    \"parameters\": {","        \"bird\": {","            \"name\": \"Swift\",","            \"age\": 42","        },","        \"times\": 42,","        \"emoji\": false","    }","}"]},{"anchor":"Defining-a-Parameter-outside-of-a-Handler","level":3,"type":"heading","text":"Defining a Parameter outside of a Handler"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Some middleware types and protocols can expose parameters as part of the endpoint defining characteristics. E.g. RESTful and OpenAPI based APIs use the URI path to define endpoints including variables such as "},{"type":"codeVoice","code":"\/birds\/BIRD_ID"},{"type":"text","text":" to identify the requested "},{"type":"codeVoice","code":"Bird"},{"type":"text","text":" by its identifier "},{"type":"codeVoice","code":"BIRD_ID"},{"type":"text","text":"."},{"type":"text","text":" "},{"type":"text","text":"A "},{"type":"codeVoice","code":"@PathParameter"},{"type":"text","text":" property used as a path parameter within a "},{"type":"codeVoice","code":"Component"},{"type":"text","text":" defined outside of a "},{"type":"codeVoice","code":"Handler"},{"type":"text","text":" (e.g.: as part of a "},{"type":"codeVoice","code":"Group"},{"type":"text","text":") can be inferred to be a parameter in the URI path for some HTTP based web API exporters."}]},{"type":"codeListing","syntax":"swift","code":["struct Bird: Identifiable {","    var id: Int","    var name: String","    var age: Int","}","","struct ExampleBirdHandler: Handler {","    \/\/ This was passed in from the outside this is automatically used as a path parameter by web API exporter that support path parameters","    @Parameter var birdID: Bird.ID","","    \/\/ ...","}","","struct ExampleNestHandler: Handler {","    \/\/ This was passed in from the outside this is automatically used as a path parameter by web API exporter that support path parameters","    @Parameter var birdID: Bird.ID","    \/\/ Exposed as a query parameter if supported by a web API exporter","    @Parameter var nestName: String?","","    \/\/ ...","}","","struct TestComponent: Component {","    @PathParameter var birdID: Bird.ID","","    var content: some Component {","        Group(\"api\", \"birds\", $birdID) {","            ExampleBirdHandler(birdID: $birdID)","            Group(\"nests\") { ","                ExampleNestHandler(birdID: $birdID)","            }","        }","    }","}"]},{"anchor":"Explicit-Options","level":2,"type":"heading","text":"Explicit Options"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Apodini web API exporters try to infer the best possible form of representing parameters."},{"type":"text","text":" "},{"type":"text","text":"However, someweb API exporters may want to provide some additional customization, so that developers may forego the inference, and provide a strategy explicitely."},{"type":"text","text":" "},{"type":"text","text":"The values of these options can be defined by the web API exporters, and each web API exporters can read for the options specific to itself."},{"type":"text","text":" "},{"type":"text","text":"For example: for HTTP JSON Based APIs, the exporter might want to allow the user to specify if a parameter is supposed to be retrieved from:"}]},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"URI Path"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"HTTP Body"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"Query Params"}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"etc."}]}]}]},{"anchor":"Defining-Options","level":3,"type":"heading","text":"Defining Options"},{"type":"paragraph","inlineContent":[{"type":"text","text":"The exporter can define it’s own options, by defining the type of the options, the key to identify the option and a helper to make it work seamlessly with the Parameter Property Wrapper:"}]},{"type":"codeListing","syntax":"swift","code":["\/\/ Define a Type","public enum HTTPParameterMode: PropertyOption {","    case body","    case path","    case query","}","","\/\/ Define a Key","extension PropertyOptionKey where Property == ParameterOptionNameSpace, Option == HTTPParameterMode {","    static let http = PropertyOptionKey<ParameterOptionNameSpace, HTTPParameterMode>()","}","","\/\/ Add helper to register it to a Parameter Property Wrapper","extension AnyPropertyOption where Property == ParameterOptionNameSpace {","    public static func http(_ mode: HTTPParameterMode) -> AnyPropertyOption<ParameterOptionNameSpace> {","        return AnyPropertyOption(key: .http, value: mode)","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"With the new definition of "},{"type":"codeVoice","code":"HTTPParameterMode"},{"type":"text","text":" the developer may now define explicitely to the exporter how they would like their parameters to be exported:"}]},{"type":"codeListing","syntax":"swift","code":["struct ExampleHandler: Handler {","    @Parameter(.http(.body)) ","    var times: Int","    ","    \/\/ Can define different handling for different exporters","    @Parameter(.http(.path))","    var id: UUID = true","","    \/\/ ...","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This has the additional benefit of making the options for each exporter explicit and clear in code, and therefore more readable, while at the same time keeping the exporters and the DSL decoupled."}]},{"anchor":"Reading-the-Options","level":3,"type":"heading","text":"Reading the Options"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Each exporter then can read the use of it’s options in the property by using the key:"}]},{"type":"codeListing","syntax":"swift","code":["extension Parameter {","    internal func httpMode() -> HTTPParameterMode {","        if let mode = option(\\.http) {","            return mode","        }","        ","        \/\/ infer mode based on other heuristics","    }","}"]}]}],"references":{"doc://Apodini/documentation/Apodini/Environment":{"role":"symbol","title":"Environment","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Environment"}],"abstract":[{"type":"text","text":"A property wrapper to inject pre-defined values  to a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/Apodini\/documentation\/Apodini\/Component"},{"type":"text","text":"."}],"identifier":"doc:\/\/Apodini\/documentation\/Apodini\/Environment","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Environment"}],"url":"\/documentation\/apodini\/environment"},"doc://Apodini/documentation/Apodini/Parameter":{"role":"symbol","title":"Parameter","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"Parameter"}],"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"@Parameter"},{"type":"text","text":" property wrapper can be used to express input in "},{"type":"codeVoice","code":"Components"}],"identifier":"doc:\/\/Apodini\/documentation\/Apodini\/Parameter","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"Parameter"}],"url":"\/documentation\/apodini\/parameter"},"doc://Apodini/documentation/Apodini/Parameter/id-90me2":{"role":"symbol","title":"id","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"id"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UUID","preciseIdentifier":"s:10Foundation4UUIDV"}],"abstract":[],"identifier":"doc:\/\/Apodini\/documentation\/Apodini\/Parameter\/id-90me2","kind":"symbol","type":"topic","url":"\/documentation\/apodini\/parameter\/id-90me2"},"doc://Apodini/documentation/Apodini/Parameter/init(wrappedValue:_:_:)":{"role":"symbol","title":"init(wrappedValue:_:_:)","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"("},{"kind":"externalParam","text":"